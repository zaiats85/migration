.header-main-menu .main-category {
  //padding: 2px 0;

  &:hover .show-menu { display: block }
  &:hover .dropdown-link:before { display: inline-block }

  a:focus { text-decoration: none }
  a:hover { color: $main-category-a-hover-color }
  a:hover .menu-container { display: block }

  a.dropdown-link { position: relative }
  a.dropdown-link:before {
    position: absolute;
    margin-right: -3px;
    bottom: -6px;
    right: 50%;
    display: none;
    border-right: 7px solid transparent;
    border-left: 7px solid transparent;
    border-bottom: 7px solid transparent;
    content: ''
  }

  //&:first-child { border-top: 1px $main-category-media-border solid }
  //&:last-child { border-bottom:0 }

  .show-menu { display:none }

  &:last-child .menu-container {
    left:0;
    right:auto
  }

  a {
    font-family:Reforma;
    //font-size:23px;
    font-size:1.642857em;
    color:$main-category-a-color
  }

  .menu-container {
    display:block;
    left:auto;
    right:0;
    -webkit-border-radius:0;
    -moz-border-radius:0;
    -ms-border-radius:0;
    -o-border-radius:0;
    border-radius:0;
    box-shadow:4px 4px 0 rgba(0,0,0,.29);
    position:absolute;
    z-index:10;
    color:$menu-container-color;
    text-align:right;
    padding:20px 0;
    margin:0;
    border:0;

    .sub-category {
      display:table-cell;
      white-space:nowrap;
      border-right:1px solid $sub-category-border
    }

    .sub-category:first-child .box { border-right:0 }

    .sub-category h3 {
      margin:0 0 8px;
      color:$sub-category-h3-color;
      //font-size:21px;
      font-size:1.5em;
      font-weight:700;
      border-bottom:1px solid $sub-category-h3-border
    }

    .sub-category .end-category { list-style:none }

    .sub-category .end-category li {
      margin-right:-60px;
      padding:4px 35px 4px 15px;
    }

    .sub-category .end-category li:before {
      content:"\203A \0020 ";
      font-size: 1.5em;
      font-family: alef;
    }

    .sub-category .end-category a {
      color:$end-category-a-color;
      //font-size:18px;
      font-size:1.285714em;
      font-weight:700
    }

    .sub-category .end-category a:hover { border-bottom:0 }

    .sub-category .box {
      padding-right:20px;
      margin-left:20px
    }
  }
}

@media (min-width:992px) {
  .header-main-menu { width:100% }

  .header-main-menu .main-category {
    &:last-child {
      width:1px;
      white-space:nowrap
    }

    &.plastic-surgery {
      a.active .underline, a:hover .underline {
        border-bottom:4px $plastic-surgery-hover-border solid;
        padding-bottom:5px
      }
        a:hover { color:$plastic-surgery-hover }
      .menu-container { background:$plastic-surgery-container-background }
      .menu-container:before { border-bottom-color:$plastic-surgery-container-before-border }
      .menu-container .sub-category h3 a:hover { opacity:.7 }
      .menu-container .end-category li:hover { background-color:$plastic-surgery-li-hover-background }
